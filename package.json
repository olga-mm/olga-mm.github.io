    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-indent-empty-lines": "^1.0.2",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-vue": "^9.9.0",
    "nuxt": "^3.8.2",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.4.7",
    "autoprefixer": "^10.4.13",
    "axios": "^1.3.4",
    "axios-auth-refresh": "^3.3.6",
    "dotenv": "^16.0.3",
    "mem": "^9.0.2",
