{
  "private": true,
  "engines": {
    "node": ">=18",
    "npm": ">=10"
  },
  "scripts": {
    "build": "nuxt build --dotenv env/.env.develop",
    "build:staging": "nuxt build --dotenv env/.env.staging",
    "build:production": "nuxt build --dotenv env/.env.production",
    "dev": "nuxt dev --dotenv env/.env.local",
    "generate": "nuxt generate --dotenv env/.env.develop",
    "generate:staging": "nuxt generate --dotenv env/.env.staging",
    "generate:production": "nuxt generate --dotenv env/.env.production",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .eslintignore .",
    "lint-fix": "eslint --ext .js,.ts,.vue --ignore-path .eslintignore . --fix"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.52.0",
